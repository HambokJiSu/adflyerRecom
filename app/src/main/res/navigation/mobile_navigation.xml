<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.adflyerrecom.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_about"
        android:name="com.example.adflyerrecom.ui.about.AboutFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_about" />

    <fragment
        android:id="@+id/navigation_review"
        android:name="com.example.adflyerrecom.ui.review.ReviewFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_review" />

    <fragment
        android:id="@+id/navigation_recom_food"
        android:name="com.example.adflyerrecom.ui.recom_food.RecomfoodFragment"
        android:label="@string/title_recom_food"
        tools:layout="@layout/fragment_recom_food" />

    <fragment
        android:id="@+id/navigation_recom_fit"
        android:name="com.example.adflyerrecom.ui.recom_fit.RecomfitFragment"
        android:label="@string/title_recom_fit"
        tools:layout="@layout/fragment_recom_fit" />
</navigation>